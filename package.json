{
    "name": "data-driven-parameters",
    "version": "0.1.0",
    "private": true,
    "homepage": "https://tableau.github.io/extension-data-driven-parameters/",
    "dependencies": {
        "@tableau/tableau-ui": "^2.0.1",
        "@types/react-router-dom": "^4.2.7",
        "react": "^16.4.1",
        "react-dom": "^16.4.1",
        "react-router-dom": "^4.3.1",
        "react-scripts-ts": "2.16.0"
    },
    "scripts": {
        "start": "react-scripts-ts start",
        "build": "react-scripts-ts build && (if exist rd /s /q docs) && move build docs",
        "release": "set PUBLIC_URL=/extension-data-driven-parameters/2.0.0&&react-scripts-ts build && (if exist \"docs/2.0.0\" rd /s /q \"docs/2.0.0\") && move build \"docs/2.0.0\"",
        "test": "react-scripts-ts test --env=jsdom",
        "eject": "react-scripts-ts eject"
    },
    "devDependencies": {
        "@types/jest": "^23.1.5",
        "@types/node": "^10.5.2",
        "@types/react": "^16.4.6",
        "@types/react-dom": "^16.0.6",
        "typescript": "^2.9.2"
    }
}